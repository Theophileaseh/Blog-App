


<%= render partial: "users/user" , locals: { user: @user } %>
 
<div class="post-container">
  <% @posts.each_with_index do |post, index| %>
    <%= render post %>
    

      <ul>
        <% post.most_recent_five_comments.each do |comment| %>
          <%= render comment %>

          <% end %>
      </ul>
      <% end %>
</div>

<div class="new-post">
<%= render "form", post: @post %>
</div>